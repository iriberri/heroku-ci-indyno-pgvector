{
  "name": "heroku-ci-indyno-pgvector",
  "description": "A sample app that installs pgvector for in-dyno PG in Heroku CI",
  "buildpacks": [
    {
      "url": "heroku/ruby"
    }
  ],
  "environments": {
    "test": {
      "addons": ["heroku-postgresql:in-dyno"],
      "scripts": {
      "test-setup": "bash install-pgvector.sh",
      "test": "psql $DATABASE_URL -c \"select * from pg_available_extensions();\""
      }
  }
}
}
